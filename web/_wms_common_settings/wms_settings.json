{
	/*
	 * the url of the public facing service; since the public WMS is proxied, it is needed to generate a valid Capabilities document
	 */
	PublicAccessUrl: 'http://localhost/hgis_wms/public.ashx/',
	
	/*
	 * the url of the backend service
	 */
	BackendServiceUrl: 'http://localhost/hgis_wms/internal.ashx/',
	
	/*
	 * data folder for the manifold wms driver
	 */
	WmsDataFolder: 'F:\\hgisdata\\manifold',
	
	/*
	 * Name of the map component used to render the data
	 */
	MapComponent: 'wms.hgis.cartomatic.pl',
	
	/*
	 * path to the bitmap to be used as a watermark; must be a file that can be opened by the Bitmap object
	 */
	Watermark: '../../resx/watermark/watermark.png'
}